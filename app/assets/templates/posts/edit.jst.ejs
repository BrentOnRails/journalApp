<h2>Edit</h2>

<form class="edit">
  <label for="post_title">Title</label>
  <input type="text" name="post[title]" id="post_title"
  value="<%= post.get("title") %>"> <br>
  <label for="post_body">Body </label>
  <textarea name="post[body]" id="post_body"><%= post.get("body") %></textarea>
  <br>
  <input type="submit">
</form>

